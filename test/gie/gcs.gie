expect failure invalid_op_illegal_arg_value

echo cube_face:: testing missing index (2/2)...
operation +proj=gcs +cube_index=012345 +cube_face=6
expect failure invalid_op_illegal_arg_value

echo cube_face:: testing bad index (1/3)...
operation +proj=gcs +cube_index=123456 +cube_face=-1
expect failure invalid_op_illegal_arg_value

echo cube_face:: testing bad index (2/3)...
operation +proj=gcs +cube_index=123456 +cube_face=7
expect failure invalid_op_illegal_arg_value

echo cube_face:: testing bad index (3/3)...
operation +proj=gcs +cube_index=123456 +cube_face=a
expect failure invalid_op_illegal_arg_value


echo cube_net:: out-of-range (1/3)...
